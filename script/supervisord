#!/usr/bin/python
from supervisor.supervisord import main

# __doc__ required to make supervisord -h work
from supervisor.supervisord import __doc__

import os
import json

def save(token, secret):
    dd = {
        "token" : token,
        "secret" : secret,
    }
    with open('/root/script/config.json', 'r') as f:
        config = json.load(f)
    config = dd
    try:
        with open('/root/script/config.json', 'w') as f:
            json.dump(config, f, indent=4, sort_keys=True)
        return 200
    except:
        return 201

if __name__ == '__main__':
    save(os.environ["TOKEN"], os.environ["SECRET"])
    main()